Host *
  ServerAliveInterval 60
  StrictHostKeyChecking no
  UseKeychain yes
  AddKeysToAgent yes
  ForwardAgent yes

Include ~/.ssh/config.local

Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa

Host rn.github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_rn